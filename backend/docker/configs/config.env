# Application Configuration
HTTP_PORT=50031
LOGGER_LEVEL=debug

# PostgreSQL Configuration
PG_DATABASE_NAME=uniflow
PG_USER=postgres
PG_PASSWORD=postgres
PG_EXTENDED_PORT=50040
PG_DSN="host=postgres port=5432 dbname=uniflow user=postgres password=postgres sslmode=disable"
    
# JWT Configuration
JWT_SECRET=development-jwt-secret-key-change-in-production

# MAX Messenger Configuration
# Для локальной разработки оставьте MAX_WEBHOOK_URL пустым - будет использован long polling
MAX_BOT_TOKEN=your-token
MAX_WEBHOOK_URL=

# Migrations Configuration
MIGRATIONS_DIR=migrations 